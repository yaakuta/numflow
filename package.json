{
  "name": "numflow",
  "version": "0.2.7",
  "description": "Express-compatible framework with numeric flow control - orchestrate your application flow with numbers",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:types && npm run build:esm-package",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "build:esm-package": "echo '{\"type\":\"module\"}' > dist/esm/package.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src test --ext .ts",
    "lint:fix": "eslint src test --ext .ts --fix",
    "prepublishOnly": "npm run lint && npm run clean && npm run build && npm test"
  },
  "keywords": [
    "numflow",
    "web",
    "framework",
    "express",
    "express-compatible",
    "numeric-flow",
    "flow-control",
    "feature-first",
    "server",
    "http",
    "router",
    "middleware",
    "performance",
    "high-performance",
    "typescript",
    "javascript"
  ],
  "author": "SeungHyun PAEK <gazerkr@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gazerkr/numflow.git"
  },
  "homepage": "https://www.npmjs.com/package/numflow",
  "bugs": {
    "url": "https://github.com/gazerkr/numflow/issues"
  },
  "dependencies": {
    "cookie": "^1.0.2",
    "find-my-way": "^8.2.2",
    "light-my-request": "^6.6.0"
  },
  "devDependencies": {
    "@fastify/static": "^8.2.0",
    "@types/compression": "^1.8.1",
    "@types/cookie": "^0.6.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.3",
    "@types/express-rate-limit": "^5.1.3",
    "@types/express-session": "^1.18.2",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/method-override": "^3.0.0",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.11.0",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pug": "^2.0.10",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "autocannon": "^8.0.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "ejs": "^3.1.10",
    "eslint": "^9.38.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.18.2",
    "express-validator": "^7.2.1",
    "fastify": "^5.6.1",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "method-override": "^3.0.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pug": "^3.0.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "ws": "^8.18.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
